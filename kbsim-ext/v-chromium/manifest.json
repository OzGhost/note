{
    "manifest_version": 3,
    "name": "kbsim-c",
    "version": "1.3",
    "description": "nope!",
    "icons": {
        "48": "farm-48.png"
    },
    "permissions": ["activeTab", "storage"],
    "content_scripts": [
        {
            "matches": ["https://data.mo.ee/"],
            "js": ["ground.js"]
        }
    ],
    "options_ui": {
